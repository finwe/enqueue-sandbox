{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.4",
        "laravel/tinker": "~1.0",
        "enqueue/fs": "*@dev",
        "enqueue/simple-client": "*@dev",
        "enqueue/laravel-queue": "^0.6@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../dev/pkg/test"
        },
        {
            "type": "path",
            "url": "../dev/pkg/enqueue"
        },
        {
            "type": "path",
            "url": "../dev/pkg/stomp"
        },
        {
            "type": "path",
            "url": "../dev/pkg/amqp-ext"
        },
        {
            "type": "path",
            "url": "../dev/pkg/redis"
        },
        {
            "type": "path",
            "url": "../dev/pkg/enqueue-bundle"
        },
        {
            "type": "path",
            "url": "../dev/pkg/job-queue"
        },
        {
            "type": "path",
            "url": "../dev/pkg/fs"
        },
        {
            "type": "path",
            "url": "../dev/pkg/null"
        },
        {
            "type": "path",
            "url": "../dev/pkg/dbal"
        },
        {
            "type": "path",
            "url": "../dev/pkg/sqs"
        },
        {
            "type": "path",
            "url": "../dev/pkg/pheanstalk"
        },
        {
            "type": "path",
            "url": "../dev/pkg/gearman"
        },
        {
            "type": "path",
            "url": "../dev/pkg/rdkafka"
        },
        {
            "type": "path",
            "url": "../dev/pkg/simple-client"
        },
        {
            "type": "path",
            "url": "../dev/pkg/async-event-dispatcher"
        }
    ]
}
